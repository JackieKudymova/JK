#include <stdio.h>

int max_0(int *a, int n) {
	int j;
	int s;
	s=a[1];
	for (j=0; j<n; j=j+1) {
		if (a[j]>s) {
			s=a[j];
		}
	}
	return s;
}
int min_1(int *a, int n) {
	int j;
	int s;
	s=a[1];
	for (j=0; j<n; j=j+1) {
		if (a[j]<s) {
			s=a[j];
		}
	}
	return s;
}
int diff_2(int *a, int n) {
	int diff;
	diff = max_0(a,n) - min_1(a,n);
	return diff;
}
int sum_3(int *a, int n) {
	int s;
	int j;
	j=0;
	s=0;
	while (a[j]> min_1(a,n)) {
		s=s+a[j];
		j=j+1;
	}
	return s;
}

int main(){
	int k;
	int i=0;
	int a[100];
	int c;
	scanf("%d",&k);
	while(1){
		c=scanf("%d",a+i);
		if(c==-1){
			break;
		}
		i++;
}
	switch(k) {
		case 0:
			printf("%d", max_0(a,i));
			break;
		case 1:
			printf("%d", min_1(a,i));
			break;
		case 2:
			printf("%d", diff_2(a,i));
			break;
		case 3:
			printf("%d", sum_3(a,i));
			break;
		}
	return 0;
}
